void sort_vetor (int* v, size_t len)\par
\{\par
    int p = 0, i;\par
\par
    while(p < (len - 1))\par
    \{\par
        i = p + 1;\par
        while(i < len)\par
        \{\par
            if(*(v + p) < *(v + i))\par
            \{\par
                int tmp = *(v + p);\par
                *(v + p) = *(v + i);\par
                *(v + i) = tmp;\par
            \}\par
            i++;\par
        \}\par
        p++;\par
    \}\par
\}\par
